name:       DeploymentManager
type:       Transition
flow:       AdminSmartFlow
foreach:    TenantAdmin|DeployEvent,FlowAdmin|ListDeployments,TenantAdmin|InternalDeployEvent
actions:
    - name:     deployJar
      method:   deployJar
      foreach:  TenantAdmin|DeployEvent,TenantAdmin|InternalDeployEvent
      from:     active
      to:       active
    - name:     retrieveDeployments
      method:   retrieveDeployments
      foreach:  FlowAdmin|ListDeployments
      from:     active
      to:       active
services:
    - name:     deployJarService
      method:   deployJarService

